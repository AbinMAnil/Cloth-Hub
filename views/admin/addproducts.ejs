 // get sub catagory with ajax call
 function getSub() {
 
   document.getElementById("subCatSel").innerText = ""
 
   var data = {
     mainCatagory: document.getElementById("mainCat").value
   }
 
 
   if (data.mainCatagory != "") {
     $.ajax({
       url: '/admin/getsubCat',
       data: data,
       method: "post",
       success: (data) => {
 
         var select = document.getElementById("subCatSel");
         for (var i = 0; i < data.val.length; i++) {
           const newOption = document.createElement('option');
           newOption.innerText = data.val[i];
           select.appendChild(newOption)
         }
       }
     })
   }
 }

